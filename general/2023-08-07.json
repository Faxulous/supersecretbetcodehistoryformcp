        },
        "thread_ts": "1691409759.992119",
        "reply_count": 1,
        "reply_users_count": 1,
        "latest_reply": "1691420479.437859",
        "reply_users": [
            "U9JHLMZB4"
        ],
        "replies": [
            {
                "user": "U9JHLMZB4",
                "ts": "1691420479.437859"
