                            },
                            {
                                "type": "text",
                                "text": "\n\n"
                            },
                            {
                                "type": "text",
                                "text": "df = wr.athena.read_sql_query(sql=my_sql_text, database=\"bf\", boto3_session=session, keep_files=False)",
