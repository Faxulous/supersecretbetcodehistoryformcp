    {
        "user": "U4H19D1D2",
        "type": "message",
        "ts": "1517499502.000428",
        "text": "When markets are closed by betfair, lightweight leaves them in the cache but there is no issue with this and it’s probably wise to leave them as results may be amended after a close. Streams aren’t additive, you have to send a new market filter request, however new markets which meet the filter are added to the stream by betfair ",
        "team": "T4G9NBD2M",
        "user_team": "T4G9NBD2M",
        "source_team": "T4G9NBD2M",
        "user_profile": {
            "avatar_hash": "137c5a3ef323",
            "image_72": "https:\/\/avatars.slack-edge.com\/2024-03-15\/6819395173841_137c5a3ef323f1944a1a_72.png",
            "first_name": "liam",
            "real_name": "liam",
            "display_name": "liam",
            "team": "T4G9NBD2M",
            "name": "liam",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "tXQEy",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "When markets are closed by betfair, lightweight leaves them in the cache but there is no issue with this and it’s probably wise to leave them as results may be amended after a close. Streams aren’t additive, you have to send a new market filter request, however new markets which meet the filter are added to the stream by betfair"
                            }
                        ]
                    }
                ]
            }
        ]
    },
