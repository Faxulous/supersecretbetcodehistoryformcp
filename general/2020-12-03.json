            "real_name": "liam",
            "display_name": "liam",
            "team": "T4G9NBD2M",
            "name": "liam",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "51e",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "process_orders will contain all orders but only called on a order stream update or every 0.25s when there are live orders, in your example you could do something like this\n"
                            }
                        ]
                    },
                    {
                        "type": "rich_text_preformatted",
                        "elements": [
                            {
                                "type": "text",
                                "text": "    def process_market_book(self, market, market_book):\n        for order in market.blotter:\n            if order.status == OrderStatus.EXECUTION_COMPLETE:\n                if order.size_remaining == 0 and order.elapsed_seconds > (60 * 10):\n                    # place closing order..\n                    pass"
                            }
                        ]
                    }
                ]
            }
        ]
