                    {
                        "type": "rich_text_preformatted",
                        "elements": [
                            {
                                "type": "text",
                                "text": "def seconds_to_start(self) -> float:\n    msd = self.market_start_datetime\n    if msd == datetime.datetime(2024, 3, 13, 13, 30):\n        msd += datetime.timedelta(minutes=15)\n    if msd == datetime.datetime(2024, 3, 13, 14, 10):\n        msd += datetime.timedelta(minutes=20)\n    if msd == datetime.datetime(2024, 3, 13, 14, 50):\n        msd += datetime.timedelta(minutes=25)\n    if msd == datetime.datetime(2024, 3, 13, 15, 30):\n        msd += datetime.timedelta(minutes=30)\n    return (msd - datetime.datetime.utcnow()).total_seconds()\n\nsetattr(Market, \"seconds_to_start\", property(seconds_to_start))"
                            }
                        ],
                        "border": 0
                    }
                ]
            }
