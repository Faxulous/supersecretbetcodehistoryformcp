                        "elements": [
                            {
                                "type": "text",
                                "text": "Not sure if this will work as each process may need its own "
                            },
                            {
                                "type": "text",
                                "text": "trading",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": " object but this is the basic pattern I follow for any parallelisation job in Python:\n\n"
                            }
                        ]
                    },
                    {
                        "type": "rich_text_preformatted",
                        "elements": [
                            {
                                "type": "text",
                                "text": "import os\nfrom concurrent import futures\nfrom tqdm import tqdm\n\nwith futures.ProcessPoolExecutor(max_workers=os.cpu_count()) as pool:\n    all_futures = [pool.submit(trading.historic.download_file, file_path=file) for file in file_list]\n    for future in tqdm(futures.as_completed(all_futures), total=len(all_futures)):\n        future.result()"
                            }
