        },
        "thread_ts": "1719245744.347629",
        "parent_user_id": "UBS7QANF3",
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "y5\/W\/",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "I do a fiddle in "
                            },
                            {
                                "type": "text",
                                "text": "check_market_book() ",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": "where I initially set a default of 999999\n"
                            }
                        ]
                    },
                    {
                        "type": "rich_text_preformatted",
                        "elements": [
                            {
                                "type": "text",
                                "text": "if market.context[self.ctxname][\"skipToSeconds\"] != 999999 \\\n        and realSecsToGo > market.context[self.ctxname][\"skipToSeconds\"] :\n    # after start time and asked need to skip\n    return False"
                            }
                        ],
                        "border": 0
                    },
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "and in the strategy when finished set it with\n"
                            }
                        ]
                    },
                    {
                        "type": "rich_text_preformatted",
                        "elements": [
                            {
                                "type": "text",
                                "text": "market.context[self.ctxname][\"skipToSeconds\"] = -9999"
                            }
                        ],
                        "border": 0
                    }
                ]
            }
        ]
    },
