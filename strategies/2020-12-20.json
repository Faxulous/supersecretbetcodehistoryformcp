        "user_profile": {
            "avatar_hash": "g232999cc026",
            "image_72": "https:\/\/secure.gravatar.com\/avatar\/232999cc026888da5959f96d96d38903.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0015-72.png",
            "first_name": "",
