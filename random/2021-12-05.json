    {
        "user": "UBS7QANF3",
        "type": "message",
        "ts": "1638719812.447900",
        "client_msg_id": "C17075DD-E724-4F61-9E34-BC04CC274D95",
        "text": "Essentially there is a good deal of randomisation. Yes you'll get the benefit of both processes running in parallel but they won't be fixed to a particular vCPU (unless you configure it that way yourself) and the CPUs will be shared with everything else running on the machine",
        "team": "T4G9NBD2M",
        "user_team": "T4G9NBD2M",
        "source_team": "T4G9NBD2M",
        "user_profile": {
            "avatar_hash": "gaaf844a4a90",
            "image_72": "https:\/\/secure.gravatar.com\/avatar\/eaaf844a4a905431d83430e563b077aa.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0011-72.png",
            "first_name": "",
            "real_name": "Maurice Berk",
            "display_name": "Mo",
            "team": "T4G9NBD2M",
            "name": "maurice",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "0vk",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "Essentially there is a good deal of randomisation. Yes you'll get the benefit of both processes running in parallel but they won't be fixed to a particular vCPU (unless you configure it that way yourself) and the CPUs will be shared with everything else running on the machine"
                            }
                        ]
                    }
                ]
            }
        ]
    },
